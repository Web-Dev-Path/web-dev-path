{
  "name": "web-dev-path",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "dev": "yarn run open-browser-win && next dev -p 3010",
    "dev-mac": "yarn run open-browser-mac && next dev -p 3010",
    "open-browser-win": "start http://localhost:3010",
    "open-browser-mac": "open http://localhost:3010",
    "build": "next build",
    "start": "next start",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "html-entities": "^2.3.2",
    "next": "^15.3.2",
    "next-pwa": "^5.6.0",
    "node-mailjet": "^6.0.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.35.0",
    "sass": "^1.35.1",
    "swiper": "^11.2.6"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "prettylint": "^2.0.0",
    "webpack": "^5.88.1"
  },
  "license": "GPL-3.0",
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
